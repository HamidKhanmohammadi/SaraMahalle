<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.powrolx.saramahalle">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.INTER"/>

    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:name="project.main.G"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Dark"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">

        <activity android:name="project.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="project.activity.CityServicesActivity"></activity>
        <activity android:name="project.activity.BanksActivity"></activity>
        <activity android:name="project.activity.EstateActivity"></activity>
        <activity android:name="test.AboutActivity"></activity>
        <activity android:name="project.activity.ActivitySubmitRequest"></activity>
        <activity android:name="project.activity.ProdactDetail"></activity>
    </application>

</manifest>